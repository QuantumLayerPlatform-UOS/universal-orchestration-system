# Agent Configuration
META_AGENT_ID=meta-prompt-orchestrator
LOG_LEVEL=info

# Agent Manager Connection
AGENT_MANAGER_URL=http://agent-manager:8082

# Azure OpenAI Configuration
AZURE_OPENAI_API_KEY=your-api-key
AZURE_OPENAI_INSTANCE_NAME=your-instance-name
AZURE_OPENAI_DEPLOYMENT_NAME=your-deployment-name
AZURE_OPENAI_API_VERSION=2023-05-15

# Performance Settings
MAX_CONCURRENT_AGENTS=10
DEFAULT_AGENT_TTL=3600000
HEARTBEAT_INTERVAL=30000

# Redis Configuration (for agent state management)
REDIS_URL=redis://redis:6379
REDIS_PREFIX=meta-agent:

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090